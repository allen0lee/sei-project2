<div class="all-albums-container">
  <%# <div class="page"> %>
  <% @albums.each do |album| %>
    <div class="album-container">
      <img src="<%= album.theme_image_url %>" alt="">
      <div class="album-name">
        <a href="/photos/<%= album.name %>/<%= album.id %>">
          <%= album.name %>
        </a>
      </div>
    </div>
  <% end %>
  <%# </div> %>
</div>


<%# all photos in one album with 'upload photo' button%>


<% @photos.each do |photo|%>
    <img src="<%= photo.image_url %>" alt="">
    <a href="/photos/<%= photo.id %>"><%= photo.name %></a>
<% end %>

<form action="/photos/<%= @album_id %>/<%= @user_id %>/new">
    <button>upload photo</button>
</form>




